    "dom7": "^4.0.6",
    "file-loader": "^6.2.0",
    "firebase": "^10.7.1",
    "firebase-cli": "^1.2.0",
    "framework7": "^8.3.0",
    "framework7-icons": "^5.0.5",
    "framework7-svelte": "^8.3.0",
    "html-webpack-plugin": "^5.6.0",
    "less-loader": "^11.1.4",
    "locate-path": "^7.2.0",
    "material-icons": "^1.13.12",
    "mini-css-extract-plugin": "^2.7.7",
    "postcss-loader": "^7.3.4",
