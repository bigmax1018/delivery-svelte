    "autoprefixer": "^10.4.16",
    "cpy-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "less": "^4.2.0",
    "postcss": "^8.4.33",
    "postcss-preset-env": "^9.3.0",
    "tailwindcss": "^3.4.1",
